{
    "version": 2,
    "builds": [{
        "src": "app.js",
        "use": "@now/node-server"
    }],
    "routes": [{
            "src": "/libreria/(?<id>[^/]+)",
            "dest": "/app.js/?id=$id",
            "methods": [
                "DELETE"
            ]
        },
        {
            "src": "/libreria/(?<id>[^/]+)",
            "dest": "/app.js/?id=$id",
            "methods": [
                "PUT"
            ]
        },
        {
            "src": "/librerias_caba",
            "dest": "/app.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/librerias_caba",
            "dest": "/app.js",
            "methods": [
                "POST"
            ]
        },
        {
            "src": "/",
            "dest": "/app.js",
            "methods": [
                "GET"
            ]
        }
    ]
}